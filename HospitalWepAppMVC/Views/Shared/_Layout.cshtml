<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HospitalWepAppMVC</title>

    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />*@

    <link rel="~/theme/shortcut icon" href="#" type="image/png">
    <link href="~/theme/css/style.css" rel="stylesheet">
    <link href="~/theme/css/style-responsive.css" rel="stylesheet">
</head>

<body class="sticky-header">
    <section>
        <!-- left side start-->
        <div class="left-side sticky-left-side">

            <!--logo and iconic logo start-->
            <div class="logo">
                <a href="index.html"><img src="~/theme/images/logo.png" alt=""></a>
            </div>

            <div class="logo-icon text-center">
                <a href="index.html"><img src="~/theme/images/logo_icon.png" alt=""></a>
            </div>
            <!--logo and iconic logo end-->


            <div class="left-side-inner">

                <!-- visible to small devices only -->
                <div class="visible-xs hidden-sm hidden-md hidden-lg">
                    <div class="media logged-user">
                        <img alt="" src="~/theme/images/photos/user-avatar.png" class="media-object">
                        <div class="media-body">
                            <h4><a href="#">John Doe</a></h4>
                            <span>"Hello There..."</span>
                        </div>
                    </div>

                    <h5 class="left-nav-title">Account Information</h5>
                    <ul class="nav nav-pills nav-stacked custom-nav">
                        <li><a href="#"><i class="fa fa-user"></i> <span>Profile</span></a></li>
                        <li><a href="#"><i class="fa fa-cog"></i> <span>Settings</span></a></li>
                        <li><a href="#"><i class="fa fa-sign-out"></i> <span>Sign Out</span></a></li>
                    </ul>
                </div>

                <!--sidebar nav start-->
                <ul class="nav nav-pills nav-stacked custom-nav">
                    <li class="menu-list nav-active">
                        <a href="index.html"><i class="fa fa-home"></i> <span>الرئيسية</span></a>
                        <ul class="sub-menu-list">
                            <li><a href="index_alt.html"> حالة جديدة</a></li>
                            <li><a href="index.html">بحث</a></li>
                        </ul>
                    </li>
                    <li class="menu-list ">
                        <a href=""><i class="fa fa-laptop"></i> <span>التقارير</span></a>
                        <ul class="sub-menu-list">
                            <li class="active"><a href="blank_page.html">تقرير اجمالى الحالات</a></li>
                            <li><a href="boxed_view.html">تقرير حالات الوفيات</a></li>
                            <li><a href="leftmenu_collapsed_view.html">تقرير الاسرة</a></li>
                           

                        </ul>
                    </li>
                    <li class="menu-list">
                        <a href=""><i class="fa fa-book"></i> <span>الاعدادات</span></a>
                        <ul class="sub-menu-list">
                            <li><a href="general.html">اعدادات الاسرة</a></li>
                            <li><a href="buttons.html">اعدادات المستشفيات</a></li>
                           
                        </ul>
                    </li>
                    <li class="menu-list">
                        <a href=""><i class="fa fa-cogs"></i> <span>المستخدمين</span></a>
                        <ul class="sub-menu-list">
                            <li><a href="grids.html"> اضافة مستخدم</a></li>
                            <li><a href="gallery.html">ادارة المستخدمين</a></li>
                         

                        </ul>
                    </li>

                </ul>
                <!--sidebar nav end-->

            </div>
        </div>
        <!-- left side end-->

        @RenderBody()

    </section>

    <!-- Placed js at the end of the document so the pages load faster -->
    <script src="~/theme/js/jquery-1.10.2.min.js"></script>
    <script src="~/theme/js/jquery-ui-1.9.2.custom.min.js"></script>
    <script src="~/theme/js/jquery-migrate-1.2.1.min.js"></script>
    <script src="~/theme/js/bootstrap.min.js"></script>
    <script src="~/theme/js/modernizr.min.js"></script>
    <script src="~/theme/js/jquery.nicescroll.js"></script>

    <!--common scripts for all pages-->
    <script src="~/theme/js/scripts.js"></script>
    @RenderSection("Scripts", required: false)

